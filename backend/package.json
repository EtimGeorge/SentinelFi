{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "start": "nest start --entryFile backend/src/main",
    "start:dev": "nest start --watch --entryFile backend/src/main",
    "start:local": "nest start --debug --watch",
    "lint": "set NODE_OPTIONS=--max-old-space-size=4096 && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "postbuild": "powershell -Command \"mkdir .\\dist\\database\\ -Force\" && copy /Y .\\src\\database\\tenant-schema.sql .\\dist\\database\\tenant-schema.sql"
  },
  "dependencies": {
    "@nestjs/throttler": "^6.5.0"
  }
}

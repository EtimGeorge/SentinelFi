{
  "name": "sentinelfi-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ],
  "scripts": {
    "dev:frontend": "npm run dev -w frontend",
    "build:frontend": "npm run build -w frontend",
    "start:frontend": "npm run start -w frontend",
    "lint:frontend": "npm run lint -w frontend",
    "build:backend": "npm run build -w backend",
    "start:backend": "npm run start -w backend",
    "start:backend:dev": "npm run start:dev -w backend",
    "start:backend:local": "npm run start:local -w backend",
    "lint:backend": "npm run lint -w backend",
    "lint:ai": "temporary-files\\run_ruff_ai.bat",
    "typeorm": "ts-node -r tsconfig-paths/register --project backend/tsconfig.json ./node_modules/typeorm/cli.js",
    "typeorm:generate": "npm run typeorm migration:generate -- -d ormconfig.ts",
    "typeorm:run": "npm run typeorm migration:run -- -d ormconfig.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- -d ormconfig.ts",
    "typeorm:create": "npm run typeorm migration:create",
    "typeorm:sync": "npm run typeorm schema:sync -- -d ormconfig.ts",
    "typeorm:drop": "npm run typeorm schema:drop -- -d ormconfig.ts",
    "db:reset": "ts-node backend/scripts/db-reset.ts",
    "db:reset:client-template": "ts-node -r tsconfig-paths/register --project backend/tsconfig.json backend/scripts/db-reset-client-template.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/typeorm": "^10.0.2",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "form-data": "^4.0.5",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.562.0",
    "ms": "^2.1.3",
    "next": "^14.2.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.6.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "tailwindcss": "~3.4.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.10",
    "@nestjs/schematics": "^10.1.20",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "^14.2.35",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typeorm-ts-node-commonjs": "0.3.20",
    "typescript": "^5.9.3"
  }
}
